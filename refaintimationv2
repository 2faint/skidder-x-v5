getgenv().setsimulationradius = newcclosure(function()
    local plr = game.Players.LocalPlayer
    sethiddenproperty(plr,"MaxSimulationRadius",1000)
    sethiddenproperty(plr,"SimulationRadius",1000)
end)

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Refaintimation v3.0.0";
	Text = "Refaintimation Has Loaded! | Made by 2faint#6969 | Netless Reanimation Script";
	Duration = 3;
})

loadstring(game:GetObjects("rbxassetid://5652963729")[1].Source)()
